{
  "name": "xcsh",
  "version": "6.0.0",
  "description": "F5 Distributed Cloud Shell - Interactive CLI for F5 XC",
  "type": "module",
  "bin": {
    "xcsh": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsup",
    "build:binaries": "pkg . --compress GZip",
    "build:release": "npm run build && npm run build:binaries",
    "dev": "tsx src/index.tsx",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "security:audit": "npm audit --audit-level=high",
    "validate": "npm run typecheck && npm run lint && npm run format:check",
    "generate:domains": "tsx scripts/generate-domains.ts",
    "prebuild": "npm run generate:domains"
  },
  "keywords": [
    "f5",
    "cli",
    "terminal",
    "ink",
    "react"
  ],
  "author": "Robin Mordasiewicz",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "conf": "^13.0.1",
    "ink": "^5.2.1",
    "ink-text-input": "^6.0.0",
    "react": "^18.3.1",
    "string-width": "^7.2.0",
    "yaml": "^2.6.1"
  },
  "devDependencies": {
    "@inkjs/ui": "^2.0.0",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.14",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@yao-pkg/pkg": "^6.11.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.1.7",
    "ink-testing-library": "^4.0.0",
    "lint-staged": "^16.2.7",
    "msw": "^2.7.0",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": [
      ".specs/**/*"
    ],
    "targets": [
      "node22-linux-x64",
      "node22-linux-arm64",
      "node22-macos-x64",
      "node22-macos-arm64",
      "node22-win-x64",
      "node22-win-arm64"
    ],
    "outputPath": "binaries"
  }
}
